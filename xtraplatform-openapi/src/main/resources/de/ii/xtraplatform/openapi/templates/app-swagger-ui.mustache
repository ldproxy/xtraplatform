
<link rel="stylesheet" type="text/css" href="{{assetsPrefix}}/xtraplatform-openapi/assets/swagger-ui-5.4.2/swagger-ui.css" >
<script src="{{assetsPrefix}}/xtraplatform-openapi/assets/swagger-ui-5.4.2/swagger-ui-bundle.js" charset="UTF-8"> </script>

<script type="text/javascript">
  const HideInfoUrlPartsPlugin = () => {
    return {
      wrapComponents: {
        InfoUrl: () => () => null,
        InfoBasePath: () => () => null,
      }
    }
  }

  const ui = SwaggerUIBundle(Object.assign({
    oauth2RedirectUrl: '{{baseUri}}{{assetsPrefix}}/xtraplatform-openapi/assets/swagger-ui-5.4.2/oauth2-redirect.html',
    presets: [
      SwaggerUIBundle.presets.apis,
    ],
    plugins: [
      HideInfoUrlPartsPlugin,
    ],
    layout: "BaseLayout",{{#showCommonExtensions}}
    showCommonExtensions: true,{{/showCommonExtensions}}
  }, globalThis._swaggerUiCfg))

  if (globalThis._swaggerUiAuth) {
    ui.initOAuth(Object.assign({
      //scopeSeparator: " ",
      //scopes: "openid profile",
      //additionalQueryStringParams: {test: "hello"},
      //useBasicAuthenticationWithAccessCodeGrant: true,
      usePkceWithAuthorizationCodeGrant: true
    }, globalThis._swaggerUiAuth))
  }
</script>